for i in range(10):
    print("Enter the mid_1 and mid_2 marks")
    marks1=int(input())
    marks2=int(input())
    maximum_marks=marks2
    minimum_marks=marks2
    if marks1>marks2:
        maximum_marks=marks1
    if marks1<marks2:
        minimum_marks=marks1
    average_marks=(0.8*maximum_marks)+(0.2*minimum_marks)
    print(round(average_marks,0))
